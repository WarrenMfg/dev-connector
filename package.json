{
  "name": "dev-connector",
  "version": "1.0.0",
  "description": "Social site for developers",
  "keywords": [
    "react",
    "node",
    "express",
    "mongoDB",
    "mongoose",
    "babel",
    "webpack",
    "MERN"
  ],
  "author": "Kent Warren",
  "license": "MIT",
  "homepage": "https://github.com/WarrenMfg",
  "repository": {
    "type": "git",
    "url": "https://github.com/WarrenMfg/dev-connector"
  },
  "main": "backend/server/index.js",
  "scripts": {
    "seed": "node dist/database/seeder.js",
    "buildFrontend": "npx webpack",
    "watchFrontend": "npx webpack --watch",
    "buildBackend": "npx babel backend --out-dir dist",
    "watchBackend": "npx babel backend --watch --out-dir dist",
    "start": "node dist/server/index.js",
    "startDev": "nodemon dist/server/index.js"
  },
  "nodemonConfig": {
    "watch": [
      "backend/*"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.3",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "bcrypt": "^4.0.1",
    "express": "^4.17.1",
    "gravatar": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.10",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "serve-static": "^1.14.1",
    "validator": "^13.0.0"
  }
}
